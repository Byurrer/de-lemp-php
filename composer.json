{
  "require": {
    "php": ">=8.2.4",
    "nikic/fast-route": "^v1.3.0",
    "guzzlehttp/guzzle": "^7.8.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "./"
    }
  },
  "require-dev":{
    "squizlabs/php_codesniffer": "^3.9",
    "phpstan/phpstan": "^1.10"
  },
  "scripts": {
    "cs": "vendor/bin/phpcs --standard=PSR12 src/",
    "lint": "vendor/bin/phpstan -c'phpstan.neon' --xdebug"
  }
}